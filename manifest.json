{
  "manifest_version": 3,
  "name": "YouTube Music Liked Toast Dismiss",
  "version": "1.0.0",
  "description": "Auto-dismisses the 'Saved to liked music' toast on YouTube Music.",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://music.youtube.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
